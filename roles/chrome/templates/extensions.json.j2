{
  "ExtensionSettings": {
    "*": {
      "installation_mode": "allowed"
    }
    {% for chrome_extension in chrome_extensions %},
    "{{ chrome_extension.id }}": {
      "update_url": "https://clients2.google.com/service/update2/crx",
      "installation_mode": "normal_installed",
      "toolbar_pin": "default_pinned"
      {% if chrome_extension.json is defined %}
      ,"json_file": "{{ chrome_extension.json }}"
      {% endif %}
    }
    {% endfor %}
  }
}
